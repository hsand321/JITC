<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".GetStartedActivity">

    <ImageView
        android:id="@+id/ivStarted"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/getstarted"
        tools:layout_editor_absoluteX="-147dp"
        tools:layout_editor_absoluteY="-38dp" />

    <ImageButton
        android:id="@+id/ibStart"
        android:layout_width="210dp"
        android:layout_height="50dp"
        android:layout_marginEnd="144dp"
        android:layout_marginRight="144dp"
        android:src="@drawable/btngetstarted"
        app:layout_constraintBottom_toBottomOf="@+id/ivStarted"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ivStarted"
        app:layout_constraintVertical_bias="0.405" />


</androidx.constraintlayout.widget.ConstraintLayout>